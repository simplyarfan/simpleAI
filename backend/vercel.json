{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "fallback-secret-key-change-in-production-vercel",
    "JWT_EXPIRES_IN": "7d",
    "REFRESH_TOKEN_SECRET": "fallback-refresh-secret-change-in-production-vercel"
  }
}